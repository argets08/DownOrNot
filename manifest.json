{
  "manifest_version": 2,
  "name": "DownOrNot",
  "version": "1.0",
  "description": "Tells if a page is down for just me or everyone else",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [ {
    "js": [ "jquery-3.3.1.js", "content.js"],
    "matches": [ "<all_urls>"]
  }],
  "background": [ {
    "scripts": [ "background.js"]
  }]
}